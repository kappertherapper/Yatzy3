doctype html
html
  head
    title Lobby Side
    link(rel="stylesheet", href="/style/lobby.css")
    script. 
      setTimeout(()=>{
        location.reload();
      }, 2000);
  body
    .container
      .lobby-box
        if name
          h1 Velkommen til lobbyen, #{name}!
           ul
            each user in users
               li #{user.name}
           form(action="/logout", method="POST")
              button(type="submit") Log ud
